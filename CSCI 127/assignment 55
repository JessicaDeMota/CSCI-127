#include <iostream>
using namespace std;

int main ()
{
  float degrees;
  float celsius;
  cout << "Enter the degree you want converted to celsius: ";
  cin >> degrees;
  celsius = ((degrees-32.0) * 5.0/9.0);

  

  cout << fixed << celsius;




  return 0;  
  
}
